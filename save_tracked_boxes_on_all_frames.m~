function save_tracked_boxes_on_all_frames(vid_name)
% assumed that cachedir and datadir are loaded
% TODO: check that they are loaded

    imlist=dir([datadir vid_name '/*.png']);
    bboxes_fname = [cachedir vid_name '_bboxes.mat'];
    
    try
        load(bboxes_fname)
    catch
        disp('File not found. Are your directories right?')
        quit;
    end
  
    
    for i=1:length(imlist)
        image_name = [vid_path imlist(i).name];
        save_tracked_boxes_on_image(vid_name, image_, bboxes(i).bbox);
    end
    
end